generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Casinos {
  id           Int    @id @default(autoincrement())
  city         String
  cstate       String
  photo        String
  casinotype   String
  crapstables  String
  website      String
  tablereports String
  tripreports  String
  name         String
}

model tableReports {
  id       String @id @db.Uuid @default(dbgenerated("gen_random_uuid()"))
  title    String
  casinoName String
  casinoLocation String
  message     String
  published Boolean  @default(false)
  createdAt DateTime @default(dbgenerated("now()"))
  
}
